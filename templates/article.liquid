          <div class="w-richtext">{{ article.content }}</div>
        </div>
        <div class="content-right">
          <div class="stick-wrapper">
            <div class="subscription">
              <div class="title-large">{{ 'article.DIV_19' | t }}</div>
              <div class="news-block w-form {% if form.posted_successfully? %}udy-form-posted-successfully{% endif %}">{% form 'customer' %}
                <form id="wf-form-general-newsletter" name="wf-form-general-newsletter" method="post" class="form-2"><div udesly-add-class="form-2"></div><input type="email" class="email-field w-input" maxlength="256" name="contact[email]" placeholder="Email address" id="email" required=""><input type="submit" value="🚀" data-wait="Please wait..." class="newsletter-blue w-button">{% include 'general-forms', form %}</form>
                <div class="success-message-3 w-form-done {% if form.posted_successfully? %}udy-form-success-message{% endif %}">
                  <div>{{ 'article.DIV_20' | t }}</div>
                </div>
                <div class="error-message-3 w-form-fail {% if form.errors %}udy-form-has-errors{% endif %}">
                  <div>{% include 'form-errors' %}</div>
                </div>
              </div>{% endform %}
            </div>
            <div class="featured-articles">
              <div class="title-large">{{ 'article.DIV_21' | t }}</div>
              {% if settings.related_articles_blog != blank %}<div class="w-dyn-list">
                <div bind="e6b03c20-3d26-c272-3abf-076ca6acf45d" role="list" class="w-dyn-items">
                  {% include "articles-list", blog: settings.related_articles_blog, sort: settings.related_articles_blog_sort, reverse: settings.related_articles_blog_reverse %}{%- assign counter = 0 -%}{% for article in current_articles %}{% if counter < settings.related_articles_blog_limit %}{% if settings.related_articles_blog_enable_filter == false or article.tags contains settings.related_articles_blog_tag %}{%- assign counter = counter | plus: 1 -%}<div bind="e6b03c20-3d26-c272-3abf-076ca6acf45e" role="listitem" class="w-dyn-item">
                    <a href="{{ article.url }}" class="featured-item w-inline-block"><img src="{{ article | img_url: 'large' }}" width="90" alt="{{ article.alt }}" class="feature-image">
                      <div class="title-small">{{ article.title }}</div>
                    </a>
                  </div>{% endif %}{% endif %}{% endfor %}
                </div>
                {% unless settings.related_articles_blog != blank and blogs[settings.related_articles_blog].articles_count != 0 %}<div bind="e6b03c20-3d26-c272-3abf-076ca6acf45f" class="w-dyn-empty">
                  <div>{{ 'article.DIV_22' | t }}</div>
                </div>{% endunless %}
              </div>{% endif %}
            </div>
          </div>
        </div>
      </div>
    </div>
