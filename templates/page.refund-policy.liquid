                      <div class="div-block-310">
                        <div class="div-block-309">
                          <a href="{{ item.product.url }}" class="product-list-title">{{ item.product.title }}</a>
                          <div class="subscription"></div>
                          <div class="product-price in-card">{{ item.price | money }}</div>
                          <a href="/cart/change?line={{ forloop.index }}&amp;amp;quantity=0" class="remove-button w-inline-block" udy-el="remove-item">
                            <div>{{ 'refund-policy.DIV_12' | t }}</div>
                          </a>
                        </div>
                        <div class="div-block-311">
                          <a href="#" class="button-9 w-button" udy-el="quantity-minus">{{ 'refund-policy.A_13' | t }}</a>
                          <input class="udesly-fake-number-input quantity input" type="number" name="updates[{{ item.key }}]" id="updates_{{ item.key }}" data-line="{{ forloop.index }}" min="1" value="{{ item.quantity }}" data-inventory-policy="{{ item.variant.inventory_policy }}" data-inventory-quantity="{{ item.variant.inventory_quantity }}">
                          <a href="#" class="button-10 w-button" udy-el="quantity-plus">{{ 'refund-policy.A_14' | t }}</a>
                        </div>
                      </div>
                    </li>{% endfor %}
                  </ul>
                  
                  
