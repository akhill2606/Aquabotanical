{% layout 'password' %}
      
  <div class="div-block-122">
    {% form 'storefront_password' %}<div class="w-password-page w-form {% if form.posted_successfully? %}udy-form-posted-successfully{% endif %}">
      <form action="/.wf_auth" method="post" class="utility-page-form w-password-page"><div udesly-add-class="utility-page-form w-password-page"></div>
        <h4 class="heading-2">{{ 'password.H4_0' | t }}</h4><input type="password" autofocus="true" maxlength="256" name="password" placeholder="Enter your password" class="input w-password-page w-input"><input type="submit" value="Submit" data-wait="Please wait..." class="submit w-password-page w-button">
        <div class="w-password-page w-form-fail {% if form.errors %}udy-form-has-errors{% endif %}">
          <div>{% include 'form-errors' %}</div>
        </div>
        
        
      {% include 'general-forms', form %}</form>
    </div>{% endform %}
  </div>
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=61530d352ea7cd652527263a" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->

{% include 'notifications' %}{{ 'udesly-shopify.min.js' | asset_url | script_tag }}{{ 'option_selection.js' | shopify_asset_url | script_tag }}